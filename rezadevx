name: 3D Contribution
on:
  schedule:
    - cron: '0 0 * * *'
  workflow_dispatch:
jobs:
  build:
    runs-on: ubuntu-latest
    steps:
    - uses: actions/checkout@v4
    - name: Generate 3D
      uses: yoshi389111/github-profile-3d-contrib@v2.11.1
      with:
        username: rezadevx
        animate: true
        folder: profile-3d-contrib